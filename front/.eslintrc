{
	"parserOptions": {
		"ecmaVersion": 2018,
		"sourceType": "module",
		"ecmaFeatures": {
			"jsx": true
		}
	},
	"env": {
		"browser": true,
		"node": true
	},
	"extends": ["airbnb", "prettier"],
  "plugins": ["prettier"],
  "rules": {
		"prettier/prettier": ["error"],
		"react/prop-types": 0
  }
}
